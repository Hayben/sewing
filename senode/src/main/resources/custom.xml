<?xml version="1.0" encoding="utf-8"?>
<root>
	<attribute>
		<id>mobile</id>
		<name>移动电话号码</name>
		<rule>1[3||5|7|8]\d{9}</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>tel</id>
		<name>固定电话号码</name>
		<rule>0[0-9]{2,3}\-[2-9][0-9]{6,7}+(\-[0-9]{1,4})?</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>email</id>
		<name>电子邮件</name>
		<rule>[\w[.-]]+@[\w[.-]]+\.[\w]+</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>url</id>
		<name>网址</name>
		<rule>[a-zA-z]+://[^\s]*</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>date</id>
		<name>日期</name>
		<rule>(^(\d{4}|\d{2})(\-|\/|\.)\d{1,2}\3\d{1,2}$)|(^\d{4}年\d{1,2}月\d{1,2}日$)</rule>
		<enable>false</enable>
	</attribute>
	<attribute>
		<id>ssn</id>
		<name>身份证号码</name>
		<rule>([1-9]\d{7}((0\d)|(1[0-2]))(([0|1|2]\d)|3[0-1])\d{3})|([1-9]\d{5}[1-9]\d{3}((0\d)|(1[0-2]))(([0|1|2]\d)|3[0-1])((\d{4})|\d{3}[Xx]))</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>passport</id>
		<name>护照号</name>
		<rule>P\d{7}|G\d{8}|S\d{7,8}|D\d+|1[4,5]\d{7}</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>milo</id>
		<name>军官证号</name>
		<rule>政字第(\d{8})号</rule>
		<enable>true</enable>
	</attribute>
	<attribute>
		<id>busrid</id>
		<name>工商注册号</name>
		<rule>\d{6}0\d{8}</rule>
		<enable>true</enable>
	</attribute>
</root>
	<seed name="亚马逊" domain="amazon.cn">
		<parser host="http://www.amazon.cn">
			<!-- category -->
			<select method="jquery" key="div.nav-subcat-links a[href]" attribute="href">
			</select>
			<!-- page -->
			<select method="jquery" key="div.pageHy span a[href]" attribute="href">
			</select>
			<!-- item -->
			<select method="jquery" key="div.s-item-container div.a-spacing-none a[href]" attribute="href">
			</select>
		</parser>
	</seed>
	<seed name="1号店" domain="yhd.com">
		<parser host="http://www.yhd.com">
			<!-- category -->
			<select method="jquery" key="div.hd_sort_list dl.clearfix dd a[href]" attribute="href">
			</select>
		</parser>
		<parser host="http://list.yhd.com">
		</parser>
		<parser host="http://item.yhd.com">
			<!-- item -->
			<select method="jquery" key="div.search_item_box a[producthref]" attribute="producthref">
			</select>
		</parser>

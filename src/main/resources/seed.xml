<?xml version="1.0" encoding="utf-8"?>
<root>
	<seed name="京东商城" domain="jd.com">
		<parser host="http://www.jd.com">
			<!-- category -->
			<select method="jquery" key="div.subitems dd a[href]" attribute="href">
			</select>
		</parser>
		<parser host="http://list.jd.com">
			<!-- page -->
			<select method="jquery" key="div.p-wrap span.p-num a[href]" attribute="href">
			</select>
			<!--item-->
			<select method="jquery" key="li.gl-item div.p-name a[href]" attribute="href">
			</select>
		</parser>

	</seed>
	<seed name="当当网" domain="dangdang.com">
		<!-- category -->
		<parser host="http://www.dangdang.com">
			<select method="jquery" key="li div.e a[href]" attribute="href">
			</select>
		</parser>
		<parser host="http://category.dangdang.com">
			<!-- page -->
			<select method="jquery" key="div.paging ul li a[href]" attribute="href">
			</select>
			<!-- item -->
			<select method="jquery" key="div.content_list div.shop_box_inner p.name a[href]" attribute="href">
			</select>
		</parser>
	</seed>
